<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/home2">

    <fragment
        android:id="@+id/home2"
        android:name="com.android.example.coroutineapk.home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_populationFragment"
            app:destination="@id/populationFragment" />
    </fragment>
    <fragment
        android:id="@+id/populationFragment"
        android:name="com.android.example.coroutineapk.PopulationFragment"
        android:label="fragment_population"
        tools:layout="@layout/fragment_population" >
        <action
            android:id="@+id/action_populationFragment_to_home2"
            app:destination="@id/home2" />
    </fragment>
</navigation>